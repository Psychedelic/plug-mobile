{
  "name": "plugmobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "nodeify": "rn-nodeify --hack --install process,stream",
    "removeDeps": "rm -rf yarn.lock node_modules && cd ios && rm -rf Pods Podfile.lock && cd ..",
    "fullInstall": "yarn install && cd ios && pod install",
    "reset": "yarn removeDeps && yarn run fullInstall",
    "patch": "npx patch-package --use-yarn",
    "postinstall": "yarn nodeify && yarn patch"
  },
  "dependencies": {
    "@dfinity/agent": "^0.10.0",
    "@dfinity/principal": "^0.10.0",
    "@psychedelic/plug-controller": "0.10.0",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.0.2",
    "@react-navigation/material-top-tabs": "^6.0.2",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.6.1",
    "assert-browserify": "^2.0.0",
    "console-browserify": "^1.2.0",
    "crypto-browserify": "^3.12.0",
    "events": "^1.1.1",
    "events-browserify": "^0.0.1",
    "formik": "^2.2.9",
    "js-sha256": "^0.9.0",
    "moment": "^2.29.1",
    "patch-package": "^6.4.7",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-native": "0.66.1",
    "react-native-bip39": "^2.3.0",
    "react-native-crypto-js": "^1.0.0",
    "react-native-fetch-api": "^2.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-haptic-feedback": "^1.11.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-mmkv": "^1.2.5",
    "react-native-modal": "^13.0.0",
    "react-native-modal-selector": "^2.0.8",
    "react-native-modalize": "^2.0.8",
    "react-native-pager-view": "^5.4.4",
    "react-native-portalize": "^1.0.7",
    "react-native-process-shim": "^1.1.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "^2.2.3",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-number-format": "^4.7.3",
    "react-redux": "^7.2.4",
    "readable-stream": "npm:vite-compatible-readable-stream",
    "redux-persist": "^6.0.0",
    "rn-nodeify": "^10.3.0",
    "stream-browserify": "^3.0.0",
    "text-encoding-polyfill": "^0.6.7",
    "url": "^0.10.3",
    "util": "^0.10.4",
    "web-streams-polyfill": "^3.1.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "console": "console-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "console": "console-browserify",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}

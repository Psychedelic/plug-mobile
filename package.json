{
  "name": "Plug",
  "version": "0.1.0(65)",
  "private": true,
  "scripts": {
    "rename-build": "chmod +x ./rename-ic-agent.sh && ./rename-ic-agent.sh",
    "ic-build": "cd cargo/ic-agent && cargo lipo --release && npm run rename-build",
    "ios": "yarn run ic-build && react-native run-ios --scheme plug-dev",
    "ios:stage": "yarn run ic-build && react-native run-ios --scheme plug-stage --configuration Debug",
    "ios:production": "yarn run ic-build && react-native run-ios --scheme plug-prod --configuration Debug",
    "android": "npx react-native run-android --variant=devDebug ",
    "android:stage": "react-native run-android --variant=stageDebug ",
    "android:production": "react-native run-android --variant=productionDebug ",
    "android:build.dev": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "android:build.stage": "cd android && ./gradlew clean && ./gradlew assembleStageRelease",
    "android:build.production": "cd android && ./gradlew clean && ./gradlew bundleProductionRelease",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "nodeify": "rn-nodeify --hack --install process,stream",
    "removeDeps": "rm -rf yarn.lock node_modules && cd ios && rm -rf Pods Podfile.lock && cd ..",
    "fullInstall": "yarn install && cd ios && pod install",
    "reset": "yarn removeDeps && yarn run fullInstall",
    "patch": "npx patch-package --use-yarn",
    "postinstall": "yarn nodeify && yarn patch"
  },
  "dependencies": {
    "@babel/plugin-transform-exponentiation-operator": "^7.16.0",
    "@dfinity/principal": "^0.9.3",
    "@psychedelic/plug-mobile-controller": "0.2.8",
